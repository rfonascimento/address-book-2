{"id":"app/services/modal.address.book.contacts.config.tpl.html","dependencies":[],"generated":{"html":"<div data-fuze-address-book-loading-blocker=\"myController.loadInProgress\">\n   <form name=\"myController.form\" novalidate=\"\">\n      <div class=\"modal-header\">\n         <h5 class=\"modal-title\" data-ng-if=\"myController.isInCreation\">Add new contact</h5>\n         <h5 class=\"modal-title\" data-ng-if=\"myController.isInEdition\">Edit contact</h5>\n      </div>\n      <div class=\"modal-body\">\n         <div class=\"alert alert-danger\" role=\"alert\" data-ng-if=\"myController.getGroups().length == 0\">\n            <i class=\"fas fa-exclamation-triangle\"></i> It's not possible do add a new contact. Please <a href=\"\" data-ng-click=\"myController.createNewContact()\">create a contact</a> group to associate the contact.\n         </div>\n         <div class=\"form-group\">\n            <label for=\"fuze-address-book-contact-config-name\">Name</label>\n            <input type=\"text\" class=\"form-control\" id=\"fuze-address-book-contact-config-name\" placeholder=\"Insert contact name\" data-ng-model=\"myController.data.name\" data-ng-disabled=\"myController.isInEdition\" required=\"\">\n         </div>\n         <div class=\"form-group\">\n            <label for=\"fuze-address-book-contact-config-phone\">Phone number</label>\n            <input type=\"tel\" class=\"form-control\" id=\"fuze-address-book-contact-config-phone\" placeholder=\"Insert phone number\" data-ng-model=\"myController.data.phone\" required=\"\">\n         </div>\n         <div class=\"form-group\" data-ng-if=\"0\">\n            <label for=\"fuze-address-book-contact-config-email\">E-mail</label>\n            <input type=\"email\" class=\"form-control\" id=\"fuze-address-book-contact-config-email\" placeholder=\"Insert e-mail\" data-ng-model=\"myController.data.email\">\n         </div>\n         <div class=\"form-group\">\n            <label for=\"fuze-address-book-contact-config-group\">Group</label>\n            <select class=\"form-control\" id=\"fuze-address-book-contact-config-group\" placeholder=\"Select contact group\" data-ng-model=\"myController.data.groupId\" required=\"\">\n               <option data-ng-repeat=\"group in myController.getGroups()\" value=\"{{group.id}}\">{{ group.name }}</option>\n            </select>\n         </div>\n         <div class=\"form-group\">\n            <label for=\"fuze-address-book-contact-config-pictureUrl\">Picture</label>\n            <input type=\"text\" class=\"form-control\" id=\"fuze-address-book-contact-config-pictureUrl\" placeholder=\"Insert picture URL\" data-ng-model=\"myController.data.pictureUrl\">\n         </div>\n      </div>\n      <div class=\"modal-footer\">\n         <button class=\"btn btn-primary\" type=\"button\" data-ng-if=\"myController.isInCreation\" data-ng-click=\"myController.actionConfigContact(myController.data)\" data-ng-disabled=\"myController.form.$invalid\">Add contact</button>\n         <button class=\"btn btn-primary\" type=\"button\" data-ng-if=\"myController.isInEdition\" ng-click=\"myController.actionConfigContact(myController.data)\" data-ng-disabled=\"myController.form.$invalid\">Edit contact</button>\n         <button class=\"btn btn-secondary\" type=\"button\" data-ng-click=\"$dismiss()\">Cancel</button>\n      </div>\n   </form>\n</div>"},"hash":"8e15ade35a7a08df5d351d553b5141c0","cacheData":{}}